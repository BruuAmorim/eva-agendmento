# Configurações do EvAgendamento

# Modo de armazenamento (desenvolvimento)
# true = usa memória (não precisa de PostgreSQL)
# false = usa PostgreSQL
USE_MEMORY_STORAGE=true

# Configurações do Banco de Dados PostgreSQL (apenas se USE_MEMORY_STORAGE=false)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=evagendamento
DB_USER=postgres
DB_PASSWORD=

# Configurações da API
PORT=3000
NODE_ENV=development

# Configurações de Segurança
JWT_SECRET=sua_chave_jwt_super_segura_aqui
API_RATE_LIMIT=100

# Configurações do Frontend
FRONTEND_URL=http://localhost:3001




